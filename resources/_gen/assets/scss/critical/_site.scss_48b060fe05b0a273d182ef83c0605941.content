@-ms-viewport{width:device-width;}*,*::before,*::after{box-sizing:border-box}html{font:20px system-ui,sans-serif}body{-ms-text-size-adjust:none;-moz-text-size-adjust:none;-webkit-text-size-adjust:none;text-size-adjust:none;-ms-touch-action:manipulation;touch-action:manipulation;-webkit-overflow-scrolling:touch;-moz-osx-font-smoothing:grayscale;-webkit-font-smoothing:antialiased}body,h1,h2,h3,h4,h5,h6,p,hr,figure,blockquote,summary,ol,ul,dl,dd,pre,iframe,textarea,button,fieldset,input,select,legend{padding:0;margin:0;border:0}input,select,textarea,button,progress,meter{-moz-appearance:none;-webkit-appearance:none;appearance:none}h1,h2,h3,h5,h6,button{font:inherit}video,img,object,svg,iframe{display:block}ol,ul{list-style:none}li::before{display:block;content:"\200C";font-size:0}a,abbr{text-decoration:none;color:inherit}address{font-style:normal}textarea{resize:none}.nobr{white-space:nowrap}#hero h1{font:700 2.6rem overpass mono,monospace;letter-spacing:-.02em;color:#9000e9}